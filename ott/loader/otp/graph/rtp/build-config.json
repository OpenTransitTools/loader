{
  "transitModelTimeZone": "America/Los_Angeles",
  "areaVisibility": true,
  "maxTransferDuration": "PT30M",
  "includeEllipsoidToGeoidDifference": true,
  "multiThreadElevationCalculations": true,
  "readCachedElevations": true,
  "writeCachedElevations": true,
  "elevationBucket": {
    "accessKey" : "${NED_ACCESS}",
    "secretKey" : "${NED_SECRET}",
    "bucketName": "${NED_BUCKET}"
  },

  "osmNaming": "portland",
  "osmDefaults": {
    "osmTagMapping": "portland"
  },

  "transitFeeds": [
    "${includeFile:feeds.json}"
  ],

  "fares": {
    "type": "combine-interlined-legs"
  }
}
