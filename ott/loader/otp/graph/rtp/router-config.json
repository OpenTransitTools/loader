    "attribution": "<a href='https://rtp.trimet.org/rtp/#/route'>Regional Partners</a>",
    "layers": [
      {
        "name": "stops",
        "type": "Stop",
        "mapper": "Digitransit",
        "maxZoom": 30,
        "minZoom": 1,
        "filter": "sunday-to-sunday-service-week",
        "cacheMaxSeconds": 600
      },
      {
        "name": "areaStops",
